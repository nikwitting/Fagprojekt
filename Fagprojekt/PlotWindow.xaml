<Window x:Class="Fagprojekt.PlotWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        
        
        xmlns:oxy="http://oxyplot.org/wpf"
        
        xmlns:local="clr-namespace:Fagprojekt"
        mc:Ignorable="d"
        Title="Plot window" Height="300" Width="300">
    <Grid>

        <Grid Margin="0,103,0,0">
            <oxy:PlotView x:Name="Plot1" Margin="10,0,10,10"/>
        </Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="88" Width="272"/>


    </Grid>
</Window>
